{
  "name": "giftdibs-api",
  "version": "0.1.0",
  "description": "giftdibs API service",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc node ./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "lint": "node ./node_modules/.bin/eslint src",
    "start": "export NODE_ENV=development && node ./index.js",
    "start:watch": "export NODE_ENV=development && nodemon ./index.js",
    "test": "npm run lint && npm run coverage",
    "firebase:config": "firebase functions:config:get > .runtimeconfig.json",
    "firebase:config:win": "firebase functions:config:get | ac .runtimeconfig.json",
    "firebase:deploy": "firebase deploy",
    "firebase:serve": "npm run firebase:config && export NODE_ENV=development && firebase serve"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giftdibs/giftdibs-api.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/giftdibs/giftdibs-api/issues"
  },
  "homepage": "https://github.com/giftdibs/giftdibs-api#readme",
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "aws-sdk": "2.279.1",
    "bcrypt": "3.0.2",
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "crypto": "1.0.1",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-multipart-file-parser": "0.1.2",
    "firebase-admin": "6.1.0",
    "firebase-functions": "2.1.0",
    "jsonwebtoken": "8.3.0",
    "lodash.orderby": "4.6.0",
    "mailgun-js": "0.20.0",
    "mongoose": "5.1.7",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "randomstring": "1.1.5",
    "request": "2.87.0",
    "request-promise": "4.2.2",
    "sharp": "0.20.5",
    "validator": "10.4.0",
    "winston": "3.0.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "eslint": "5.0.1",
    "eslint-config-standard": "12.0.0-alpha.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "jasmine": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "mock-require": "3.0.2",
    "mysql": "2.16.0",
    "nodemon": "1.17.5",
    "nyc": "12.0.2"
  }
}
